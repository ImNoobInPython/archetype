				<param name="textAlignment"><enum type="alignment">center</enum></param>
			</theme>
		</theme>
	</theme>
	
	<theme name="gameshop-item-addon-limited" ref="gameshop-item-addon" allowWildcard="true">
		<param name="background"><image>ui-inputbox.background</image></param>
		<param name="border"><border>0</border></param>
		<theme name="dialoglayout" ref="dialoglayout">
			<param name="border"><border>10,5,10,5</border></param>
			<param name="defaultGap"><dimension>40,2</dimension></param>
			<theme name="limited-time" ref="label">
				<param name="textAlignment"><enum type="alignment">center</enum></param>
			</theme>
			<theme name="label-special" ref="-defaults">
				<param name="border"><border>0,0</border></param>
				<param name="background"><image>shop-item-limited.background</image></param>
				<param name="minWidth"><int>57</int></param>
				<param name="minHeight"><int>57</int></param>
			</theme>
		</theme>
	</theme>
	
	<theme name="gameshop-item-addon-seasonal" ref="gameshop-item-addon-limited" allowWildcard="true">
		<theme name="dialoglayout" ref="dialoglayout">
			<theme name="label-special" ref="-defaults">
				<param name="background"><image>shop-item-seasonal.background</image></param>
				<param name="border"><border>20</border></param>
			</theme>
		</theme>
	</theme>
	
	<theme name="gameshop-item-addon-sale" ref="gameshop-item-addon-limited" allowWildcard="true">
		<theme name="dialoglayout" ref="dialoglayout">
			<theme name="label-special" ref="-defaults">
				<param name="background"><image>shop-item-sale.background</image></param>
			</theme>
		</theme>
	</theme>
	
	<theme name="gameshop-item-addon-new" ref="gameshop-item-addon-limited" allowWildcard="true">
		<theme name="dialoglayout" ref="dialoglayout">
			<theme name="label-special" ref="-defaults">
				<param name="background"><image>shop-item-new.background</image></param>
			</theme>
		</theme>
	</theme>

	<theme name="gameshop-item-limited" ref="gameshop-item" allowWildcard="true">
		<param name="background"><image>ui-popup.background</image></param>
		<theme name="label-special" ref="-defaults">
			<param name="background"><image>shop-item-limited.background</image></param>
			<param name="minWidth"><int>57</int></param>
			<param name="minHeight"><int>57</int></param>
		</theme>
		<theme name="limited-time" ref="label">
			<param name="textAlignment"><enum type="alignment">center</enum></param>
		</theme>
	</theme>

	<theme name="gameshop-item-seasonal" ref="gameshop-item-limited" allowWildcard="true">
		<theme name="label-special" ref="-defaults">
			<param name="background"><image>shop-item-seasonal.background</image></param>
		</theme>
	</theme>
	
	<theme name="gameshop-item-sale" ref="gameshop-item-limited" allowWildcard="true">
		<theme name="label-special" ref="-defaults">
			<param name="background"><image>shop-item-sale.background</image></param>
		</theme>
	</theme>
	
	<theme name="gameshop-item-new" ref="gameshop-item-limited" allowWildcard="true">
		<theme name="label-special" ref="-defaults">
			<param name="background"><image>shop-item-new.background</image></param>
		</theme>
	</theme>
</themes>
