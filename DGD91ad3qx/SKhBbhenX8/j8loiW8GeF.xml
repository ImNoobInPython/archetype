<?xml version="1.0" encoding="UTF-8"?>
<themes>

	<theme name="label-area-market" ref="-defaults">
	<!--<param name="fadeColorInactive"><color>#EBFFFFFF</color></param>
	<param name="fadeDurationActivate"><int>150</int></param>
	<param name="fadeDurationDeactivate"><int>210000</int></param>-->
		<param name="background"><image>none</image></param>
		<param name="border"><border>15,10,10,10</border></param>
		<param name="defaultGap"><dimension>5,5</dimension></param>
		<param name="smallGap"><dimension>8,8</dimension></param>
		<param name="mediumGap"><dimension>10,10</dimension></param>
		<param name="largeGap"><dimension>15,15</dimension></param>
		<param name="namedGaps"><map/></param>
		<param name="minWidth"><int>240</int></param>
		<param name="minHeight"><int>120</int></param>
		<param name="maxWidth"><int>240</int></param>
		<param name="maxHeight"><int>120</int></param>
		<theme name="label" ref="label" allowWildcard="true">
			<param name="font"><font>pb-dark</font></param>
		</theme>
		<theme name="scrollpane" ref="scrollpane">
			<theme name="label" ref="label" allowWildcard="true">
				<param name="font"><font>pb-dark</font></param>
			</theme>
		</theme>
	</theme>
	
	<theme name="gc-shop" ref="base-frame-padded" allowWildcard="true">	
	<!--<param name="fadeColorInactive"><color>#EBFFFFFF</color></param>
	<param name="fadeDurationActivate"><int>150</int></param>
	<param name="fadeDurationDeactivate"><int>210000</int></param>-->
	<param name="background"><image>Game-Corner-Window</image></param>
		<param name="border"><border>30,0,0,0</border></param>
		<theme name="dialoglayout" ref="-defaults">
			<param name="border"><border>0</border></param>
			<param name="defaultGap"><dimension>5,5</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
			<theme name="scrollpane" ref="scrollpane" allowWildcard="true">
				<param name="border"><border>10,10,10,10</border></param>
				<param name="maxWidth"><int>140</int></param>
			</theme>
		</theme>
	</theme>
	
	<theme name="market-button" ref="button" allowWildcard="true">
		<param name="border"><border>7,0</border></param>
		<param name="textAlignment"><enum type="alignment">LEFT</enum></param>
	</theme>
	
	<theme name="itemshop" ref="base-tabbed-frame">
	<!--<param name="fadeColorInactive"><color>#EBFFFFFF</color></param>-->
		<param name="background"><image>Poke-Shop-Window</image></param>
		<theme name="itemshop-dialoglayout-left" ref="-defaults">
			<param name="background"><image>none</image></param>
			<param name="border"><border>10,9,8,5</border></param>
			<param name="defaultGap"><dimension>5,5</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
		</theme>
		<theme name="dialoglayout" ref="-defaults">
			<param name="border"><border>0,3,0,0</border></param>
			<param name="defaultGap"><dimension>5,5</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>	
			<theme name="tabbedpane" ref="base-tabbed-pane">
				<param name="minWidth"><int>100</int></param>				
				<param name="background"><image>none</image></param>
				<param name="border"><border>5,14,0,0</border></param>
				<theme name="tabbox" ref="tabbox">
					<param name="border"><border>1,10,0,0</border></param>
					<param name="spacing"><int>2</int></param>
					<param name="alignment"><enum type="alignment">FILL</enum></param>
				</theme>
				<theme name="container" ref="-defaults">
					<param name="background"><image>none</image></param>
					<param name="border"><border>2,0,5,0</border></param>
					<theme name="dialoglayout" ref="-defaults">
						<param name="background"><image>none</image></param>
						<param name="minHeight"><int>100</int></param>
						<param name="border"><border>background.border</border></param>
						<param name="defaultGap"><dimension>5,5</dimension></param>
						<param name="smallGap"><dimension>8,8</dimension></param>
						<param name="mediumGap"><dimension>10,10</dimension></param>
						<param name="largeGap"><dimension>15,15</dimension></param>
						<param name="namedGaps"><map/></param>
						<theme name="scrollpane" ref="scrollpane" allowWildcard="true">
							<theme name="vscrollbar" ref="vscrollbar-white"/>
								<theme name="dialoglayout" ref="dialoglayout">
									<param name="border"><border>0,0,0,4</border></param>
									<param name="defaultGap"><dimension>5,3</dimension></param>
								</theme>
						</theme>
					</theme>
				</theme>
				<param name="inputMap"><inputMapDef>
					<action name="nextTab">CTRL TAB</action>
					<action name="prevTab">CTRL SHIFT TAB</action>
				</inputMapDef></param>
			</theme>
		</theme>
	</theme>
	
	<theme name="gameshop" ref="base-tabbed-frame">
	<!--<param name="fadeColorInactive"><color>#EBFFFFFF</color></param>-->
		<param name="background"><image>Gift-Shop-Window</image></param>
		<param name="minWidth"><int>683</int></param>
		<param name="minHeight"><int>593</int></param>
		<param name="border"><border>26,0,3,5</border></param>
		<param name="closeButtonY"><int>8</int></param>
		<theme name="title" ref="title">
			<param name="border"><border>10,49,2,0</border></param>
		</theme>
		<theme name="sortby" ref="-defaults">
			<param name="font"><font>pb-dark</font></param>
			<param name="border"><border>0,0,3,0</border></param>
		</theme>
		<theme name="points" ref="-defaults">
			<param name="background"><image>ui-inputbox.background</image></param>
			<param name="font"><font>pb-dark</font></param>
			<param name="border"><border>5</border></param>
		</theme>
		<theme name="dialoglayout" ref="dialoglayout">
			<theme name="tabbedpane" ref="base-tabbed-pane">
					<param name="border"><border>5,0,0,0</border></param>
				<theme name="tabbox" ref="tabbox">
					<theme name="tabbutton" ref="tabbutton">
						<param name="background"><image>ui-tab.background</image></param>
					</theme>
				</theme>
				<param name="minWidth"><int>250</int></param>
				<param name="background"><image>none</image></param>
				<theme name="container" ref="-defaults">
					<param name="border"><border>30,0,33,5</border></param>
					<theme name="dialoglayout" ref="-defaults">

						<theme name="dialoglayout" ref="-dialog">
							<theme name="tabbedpane" ref="tabbedpane">
								<param name="minWidth"><int>250</int></param>
								<param name="background"><image>none</image></param>
								<param name="border"><border>0,0,0,0</border></param>
								<theme name="tabbox" ref="-defaults">
									<param name="border"><border>5,0</border></param>
									<theme name="tabbutton" ref="button">
										<param name="border"><border>10,5</border></param>
									</theme>
									<param name="spacing"><int>1</int></param>
									<param name="alignment"><enum type="alignment">FILL</enum></param>
								</theme>
								<theme name="container" ref="-dialog">
									<param name="border"><border>0,9,0,0</border></param>
									<theme name="scrollpane" ref="scrollpane">
									</theme>
									<theme name="sp" ref="scrollpane">
										<theme name="vscrollbar" ref="vscrollbar-white"/>
										<theme name="hscrollbar" ref="hscrollbar"/>
									</theme>
								</theme>
							</theme>
						</theme>
						<param name="border"><border>5,5</border></param>
						<param name="defaultGap"><dimension>5,5</dimension></param>
						<param name="smallGap"><dimension>8,8</dimension></param>
						<param name="mediumGap"><dimension>10,10</dimension></param>
						<param name="largeGap"><dimension>15,15</dimension></param>
						<param name="namedGaps"><map/></param>
					</theme>
				</theme>
				<param name="inputMap"><inputMapDef>
					<action name="nextTab">CTRL TAB</action>
					<action name="prevTab">CTRL SHIFT TAB</action>
				</inputMapDef></param>
			</theme>
		</theme>
	</theme>
	
	<theme name="gameshop-item" ref="-defaults" allowWildcard="true">
		<param name="background"><image>ui-inputbox.background</image></param>
		<param name="border"><border>5,0,5,5</border></param>
		<param name="defaultGap"><dimension>0,5</dimension></param>
		<param name="smallGap"><dimension>8,8</dimension></param>
		<param name="mediumGap"><dimension>10,10</dimension></param>
		<param name="largeGap"><dimension>15,15</dimension></param>
		<param name="minWidth"><int>310</int></param>
		<param name="maxWidth"><int>310</int></param>
		<param name="namedGaps"><map/></param>
		<theme name="label" ref="-defaults">
			<param name="font"><font>pb-dark</font></param>
			<param name="minWidth"><int>227</int></param>
		</theme>
		<theme name="item-name" ref="-defaults">
			<param name="textAlignment"><enum type="alignment">center</enum></param>
			<param name="font"><font>pb-dark</font></param>
			<param name="background"><image>none</image></param>
			<param name="border"><border>3</border></param>
		</theme>
		<theme name="button" ref="button">
			<param name="minWidth"><int>80</int></param>
			<param name="minHeight"><int>30</int></param>
		</theme>
		<theme name="label-special" ref="-defaults">
			<param name="background"><image>none</image></param>
			<param name="minWidth"><int>57</int></param>
			<param name="minHeight"><int>57</int></param>
			<param name="maxWidth"><int>57</int></param>
			<param name="maxHeight"><int>57</int></param>
		</theme>
		<theme name="label-discount" ref="-defaults">
			<param name="font"><font>pb-dark</font></param>
			<param name="textAlignment"><enum type="alignment">center</enum></param>
		</theme>
	</theme>

	<theme name="gameshop-item-addon" ref="gameshop-item" allowWildcard="true">
		<param name="background"><image>ui-inputbox.background</image></param>
