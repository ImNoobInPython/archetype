		<area name="Pure-Top-Edge" xywh="0,0,20,10" splitx="L5,R5" splity="T5,B0"/>
		<area name="Pure-Bottom-Edge" xywh="0,10,20,10" splitx="L5,R5" splity="T0,B5"/>
