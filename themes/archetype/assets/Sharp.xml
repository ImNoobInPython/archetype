<themes>
	<images file="jaejGI7pIp/res/custom/Yzkkn9zM8H/Highlight-Overlay.png" filter="lineal">
		<area name="Highlight-Overlay-Asset" xywh="*" splitx="L5,R5" splity="T5,B5"/>
		<animation name="Highlight-Overlay" inset="2" timeSource="selected" if="hover | keyboardFocus">
			<frame duration="2000" ref="Highlight-Overlay-Asset" tint="#80FFFFFF"/>
