<?xml version="1.0" encoding="UTF-8"?>
<themes>
	<theme name="chat-monster-info" ref="button" allowWildcard="true">
		<param name="background"><image>none</image></param>
		<param name="font"><font>chat-blue</font></param>
		<!--<param name="textAlignment"><enum type="alignment">CENTER</enum></param>-->
		<param name="border"><border>2,0</border></param>
	</theme>

	<theme name="chat-monster-info-shiny" ref="chat-monster-info" allowWildcard="true">
		<param name="font"><font>chat-shiny</font></param>
		<!--<param name="textAlignment"><enum type="alignment">CENTER</enum></param>-->
	</theme>

	<theme name="chatresizableframe" ref="-defaults" allowWildcard="true">
		<!--<param name="fadeColorInactive"><color>#64FFFFFF</color></param>
		<param name="fadeDurationActivate"><int>150</int></param>
		<param name="fadeDurationDeactivate"><int>90000</int></param>
		<param name="border"><border>10,0,10,0</border></param>-->
		<param name="titleAreaTop"><int>5</int></param>
		<param name="titleAreaLeft"><int>5</int></param>
		<param name="titleAreaRight"><int>-5</int></param>
		<param name="titleAreaBottom"><int>45</int></param>
		<param name="hasCloseButton"><bool>false</bool></param>
		<param name="closeButtonX"><int>22</int></param>
		<param name="closeButtonY"><int>6</int></param>
		<param name="hasResizeHandle"><bool>false</bool></param>
		<param name="resizeHandleX"><int>0</int></param>
		<param name="resizeHandleY"><int>0</int></param>
		<param name="fadeDurationShow"><int>0</int></param>
		<param name="fadeDurationHide"><int>0</int></param>	
		<param name="mouseCursor"><cursor>mouseCursor.*</cursor></param>
		<theme name="title" ref="-defaults">
			<param name="textAlignment"><enum type="alignment">left</enum></param>
			<param name="font"><font>alphabeta</font></param>
		</theme>
		<theme name="closeButton" ref="-defaults"/>
		<theme name="resizeHandle" ref="-defaults"/>
	</theme>

	<theme name="chatframe-locked" ref="chatresizableframe">
		<param name="border"><border>5,5,5,5</border></param>
		<theme name="content" ref="-dialog">
			<param name="defaultGap"><dimension>0,0</dimension></param>
			<theme name="scrollpane" ref="scrollpane">
				<param name="background"><image>chat-middle-locked.background</image></param>
				<theme name="vscrollbar" ref="vscrollbar-white"/>
				<param name="border"><border>5,0,0,0</border></param>
				<theme name="textarea" ref="-defaults">
					<param name="border"><border>0,0</border></param>
					<param name="fonts"><map>
					<param name="default"><font>chat-default</font></param>
					<param name="color0"><font>chat-green</font></param>
					<param name="color1"><font>chat-default</font></param>
					<param name="color2"><font>chat-default</font></param>
					<param name="link"><font>chat-link</font></param>
					<param name="normal"><font>chat-default</font></param>
					<param name="shout"><font>chat-orange</font></param>
					<param name="whisper"><font>chat-lightgreen</font></param>
					<param name="trade"><font>chat-purple</font></param>
					<param name="global"><font>chat-teal</font></param>
					<param name="channel"><font>chat-yellow</font></param>
					<param name="team"><font>chat-brown</font></param>
					<param name="system"><font>chat-main-system</font></param>
					<param name="battle"><font>chat-main-system</font></param>
					</map></param>
					<param name="classes"><map/></param>
					<param name="images"><map>
					<param name="icon-gm"><image>icon-gm</image></param>
					<param name="icon-cm"><image>icon-cm</image></param>
					<param name="icon-hgm"><image>icon-hgm</image></param>
					<param name="icon-dev"><image>icon-dev</image></param>
					<param name="icon-sgm"><image>icon-sgm</image></param>
					<param name="icon-jgm"><image>icon-jgm</image></param>
					<param name="icon-mod"><image>icon-mod</image></param>
					<param name="icon-adm"><image>icon-admin</image></param>
					<param name="icon-friend"><image>icon7</image></param>
					</map></param>
				</theme>
			</theme>
			<theme name="top" ref="-dialog">	
				<param name="background"><image>chat-top-locked.background</image></param>
				<theme name="togglebutton-normal" ref="button" allowWildcard="true">
					<param name="background"><image>chat-tab.background</image></param>
					<param name="border"><border>7,8,9,8</border></param>
					<param name="font"><font>pb-dark-highlight-button</font></param>
				</theme>
				<theme name="togglebutton-settings" ref="togglebutton-normal" allowWildcard="true">
					<param name="overlay"><image>icon-chat-settings</image></param>
				</theme>
				<theme name="togglebutton-notification" ref="togglebutton-settings" allowWildcard="true">
					<param name="overlay"><image>icon-chat-sounds</image></param>
				</theme>
				<theme name="spritetogglebutton" ref="togglebutton-settings" allowWildcard="true">
					<param name="background"><image>chat-tab-lock.background</image></param>
					<param name="maxWidth"><int>24</int></param>
					<param name="maxHeight"><int>24</int></param>
				</theme>
				<theme name="hideButton" ref="button" allowWildcard="true">
					<param name="background"><image>frame.closebutton-chat</image></param>
					<param name="minWidth"><int>24</int></param>
					<param name="minHeight"><int>24</int></param>
				</theme>
			</theme>

			<theme name="bottom" ref="-dialog">
				<param name="background"><image>chat-bottom-locked.background</image></param>
				<param name="border"><border>5,0,0,0</border></param>
				<theme name="button" ref="button" allowWildcard="true">
					<param name="background"><image>special-ui-button.background</image></param>
					<param name="font"><font>alphabeta-highlight-button</font></param>
					<param name="textAlignment"><enum type="alignment">RIGHT</enum></param>
				</theme>
				<theme name="button-link" ref="button" allowWildcard="true"/>
				<theme name="editfield" ref="editfield">
				</theme>
				<theme name="chat-type-button" ref="button" allowWildcard="true">
					<param name="border"><border>5</border></param>
				</theme>
			</theme>
		</theme>
	</theme>

	<theme name="chat-playername" ref="button" allowWildcard="true">
		<param name="border"><border>0,0,0,0</border></param>
		<param name="background"><image>none</image></param>
		<param name="font"><font>chat-white</font></param>
		<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
	</theme>

	<theme name="chatframe" ref="chatframe-locked">
		<param name="fadeColorInactive"><color>#d3FFFFFF</color></param>
		<param name="fadeDurationActivate"><int>300</int></param>
		<param name="fadeDurationDeactivate"><int>300</int></param>
		<theme name="content" ref="content">
			<theme name="scrollpane" ref="scrollpane">
				<param name="background"><image>chat-middle.background</image></param>
			</theme>
			<theme name="top" ref="top">	
				<param name="background"><image>chat-top.background</image></param>
			</theme>
			<theme name="bottom" ref="bottom">
				<param name="background"><image>chat-bottom.background</image></param>
			</theme>
		</theme>
	</theme>

	<theme name="chatframe-hidden" ref="-defaults">
		<param name="background"><image>Chat-Minimized-Bar</image></param>
		<param name="minWidth"><int>24</int></param>
		<param name="minHeight"><int>24</int></param>
	</theme>

	<theme name="chat-tab-settings" ref="base-frame-vertical-tabs">
		<param name="background"><image>Chat-Window</image></param>
		<param name="minWidth"><int>580</int></param>
		<param name="minHeight"><int>444</int></param>
		<param name="maxWidth"><int>580</int></param>
		<param name="maxHeight"><int>444</int></param>
		<theme name="dialoglayout" ref="dialoglayout">
			<theme name="tabbedpane" ref="tabbedpane">
				<theme name="scrollControls" ref="scrollControls">
					<param name="defaultGap"><dimension>5,5</dimension></param>
					<param name="smallGap"><dimension>8,8</dimension></param>
					<param name="mediumGap"><dimension>10,10</dimension></param>
					<param name="largeGap"><dimension>15,15</dimension></param>
					<param name="namedGaps"><map>
						<param name="scrollButtons"><gap>2</gap></param>
					</map></param>
					<theme name="scrollLeft" ref="button">
						<param name="text"><string>&#xe2191;</string></param>
					</theme>
					<theme name="scrollRight" ref="button">
						<param name="text"><string>&#xe2193;</string></param>
					</theme>
				</theme>
				<theme name="container" ref="container">
					<param name="minWidth"><int>456</int></param>
                    <theme name="dialoglayout" ref="dialoglayout">
                    </theme>
                </theme>
				<param name="inputMap"><inputMapDef>
					<action name="nextTab">CTRL TAB</action>
					<action name="prevTab">CTRL SHIFT TAB</action>
				</inputMapDef></param>
			</theme>
		</theme>
	</theme>

	<theme name="chat-settings-area" ref="-defaults">
		<param name="defaultGap"><dimension>5,5</dimension></param>
		<param name="smallGap"><dimension>8,8</dimension></param>
		<param name="mediumGap"><dimension>10,10</dimension></param>
		<param name="largeGap"><dimension>15,15</dimension></param>
		<param name="namedGaps"><map/></param>
		<theme name="dialoglayout" ref="-defaults">
			<param name="background"><image>none</image></param>
			<param name="minWidth"><int>500</int></param>
			<param name="border"><border>0</border></param>
			<param name="defaultGap"><dimension>5,5</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
		</theme>
	</theme>

	<theme name="label-chat-settings-transparency" ref="label" allowWildcard="true">
		<param name="background"><image>ui-header.background</image></param>
		<param name="textAlignment"><enum type="alignment">CENTER</enum></param>
		<param name="border"><border>0,7,0,7</border></param>
		<param name="minWidth"><int>250</int></param>
		<param name="minHeight"><int>31</int></param>
	</theme>

	<theme name="label-chat-settings-title" ref="label-chat-settings-transparency" allowWildcard="true">
