			<param name="defaultGap"><dimension>5,5</dimension></param>
			<param name="smallGap"><dimension>8,8</dimension></param>
			<param name="mediumGap"><dimension>10,10</dimension></param>
			<param name="largeGap"><dimension>15,15</dimension></param>
			<param name="namedGaps"><map/></param>
			<theme name="label" ref="label">
				<param name="font"><font>pb-dark</font></param>
				<param name="background"><image>label2.background</image></param>
				<param name="border"><border>5</border></param>
				<param name="minHeight"><int>30</int></param>
				<param name="minWidth"><int>350</int></param>
			</theme>
			<theme name="button" ref="button">
				<param name="minWidth"><int>32</int></param>
				<param name="minHeight"><int>30</int></param>
			</theme>
			<theme name="text-scrollpane">
				<param name="background"><image>none</image></param>
				<param name="overlay"><image>none</image></param>
				<theme name="hscrollbar" ref="hscrollbar"/>
				<theme name="vscrollbar" ref="vscrollbar-white"/>
				<param name="border"><border>5</border></param>

				<param name="minWidth"><int>0</int></param>
				<param name="maxWidth"><int>32767</int></param>
				<param name="maxHeight"><int>32767</int></param>
				<param name="hasDragButton"><bool>false</bool></param>
				<param name="autoScrollArea"><int>5</int></param>
				<param name="autoScrollSpeed"><int>10</int></param>
				<param name="minHeight"><int>170</int></param>
				<theme name="editfield" ref="-defaults" allowWildcard="true">
					<param name="font"><font>alphabeta</font></param>
					<param name="background"><image>editfield.background</image></param>
					<param name="selection"><image>editfield.selection</image></param>
					<param name="border"><border>3</border></param>
					<param name="passwordChar"><int>35</int></param>
					<param name="maxWidth"><int>32767</int></param>
					<param name="columns"><int>63</int></param>
					<param name="cursor"><image>-editfield.cursor</image></param>
					<param name="mouseCursor"><cursor>cursor.text</cursor></param>
					<theme name="renderer" ref="-input"/>
					<param name="autocompletion-height"><int>250</int></param>
					<param name="minWidth"><int>340</int></param>
				</theme>
				<theme name="textarea" ref="-defaults">
						<param name="minWidth"><int>344</int></param>
						<param name="maxWidth"><int>344</int></param>
						<param name="minHeight"><int>100</int></param>
						<param name="fonts"><map>
							<param name="default"><font>pb-dark</font></param>
							<param name="chatdefault"><font>pb-dark</font></param>
							<param name="orange"><font>pb-dark-orange</font></param>
							<param name="link"><font>alphabeta-blue</font></param>
						</map></param>
						<param name="classes"><map/></param>
						<param name="images">
						<map>
							<param name="player"><image>ui-header.background</image></param>
							<param name="admin"><image>ui-header-alt.background</image></param>
							<param name="system"><image>label3.background</image></param>
						</map>
						</param>
					</theme>
			</theme>
		</theme>
	</theme>
</themes>
