	</theme>

	<theme name="label-chat-settings-special" ref="label-chat-settings-transparency" allowWildcard="true">
		<param name="minWidth"><int>170</int></param>
	</theme>

	<theme name="label-chat-settings-button" ref="label" allowWildcard="true">
		<param name="background"><image>none</image></param>
		<param name="minWidth"><int>0</int></param>
		<param name="border"><border>0,0,0,0</border></param>
	</theme>

